{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "components/ToggleColorMode.jsx", "components/Testimonials.jsx", "components/SitemarkIcon.jsx", "components/Pricing.jsx", "components/LogoCollection.jsx", "components/Highlights.jsx", "components/Hero.jsx", "components/Footer.jsx", "app/getLPTheme.tsx", "components/Share.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
